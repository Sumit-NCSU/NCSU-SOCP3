<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.srivassumit.referralsclient.MainActivity">

    <SeekBar
        android:id="@+id/cookQuery"
        android:layout_width="160dp"
        android:layout_height="21dp"
        android:max="100"
        android:min="0"
        android:progress="1"
        app:layout_constraintLeft_toRightOf="@+id/textView"
        android:layout_marginLeft="24dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="32dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="60dp"
        android:layout_height="21dp"
        android:layout_marginLeft="41dp"
        android:layout_marginTop="32dp"
        android:text="@string/cooking"
        android:textAlignment="textEnd"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/houseQuery"
        android:layout_width="160dp"
        android:layout_height="21dp"
        android:max="100"
        android:min="0"
        android:progress="1"
        app:layout_constraintLeft_toRightOf="@+id/textView3"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/cookQuery" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="@string/house"
        android:textAlignment="textEnd"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtCookingQV" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="74dp"
        android:layout_height="21dp"
        android:layout_marginLeft="27dp"
        android:layout_marginTop="16dp"
        android:text="@string/education"
        android:textAlignment="textEnd"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtHouseQV" />

    <SeekBar
        android:id="@+id/educateQuery"
        android:layout_width="160dp"
        android:layout_height="21dp"
        android:max="100"
        android:min="0"
        android:progress="1"
        app:layout_constraintLeft_toRightOf="@+id/textView4"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/houseQuery" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="16dp"
        android:text="@string/entertainment"
        android:textAlignment="textEnd"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtEducationQV" />

    <SeekBar
        android:id="@+id/entertainQuery"
        android:layout_width="160dp"
        android:layout_height="21dp"
        android:max="100"
        android:min="0"
        android:progress="1"
        app:layout_constraintLeft_toRightOf="@+id/textView2"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/educateQuery" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="41dp"
        android:text="@string/query"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtEntertainQV" />

    <SeekBar
        android:id="@+id/entertainAnswer"
        android:layout_width="160dp"
        android:layout_height="21dp"
        android:max="100"
        android:gravity="center"
        android:min="0"
        android:progress="1"
        app:layout_constraintLeft_toRightOf="@+id/txtEntertainV"
        android:layout_marginLeft="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="32dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/educateAnswer"
        app:layout_constraintVertical_bias="0.058" />

    <SeekBar
        android:id="@+id/cookAnswer"
        android:layout_width="160dp"
        android:layout_height="21dp"
        android:max="100"
        android:gravity="center"
        android:min="0"
        android:progress="1"
        app:layout_constraintLeft_toRightOf="@+id/txtCookingV"
        android:layout_marginLeft="40dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.608"
        android:layout_marginTop="8dp" />

    <SeekBar
        android:id="@+id/houseAnswer"
        android:layout_width="160dp"
        android:layout_height="21dp"
        android:max="100"
        android:gravity="center"
        android:min="0"
        android:progress="1"
        app:layout_constraintLeft_toRightOf="@+id/txtHouseV"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/cookAnswer" />

    <SeekBar
        android:id="@+id/educateAnswer"
        android:layout_width="160dp"
        android:layout_height="21dp"
        android:max="100"
        android:gravity="center"
        android:min="0"
        android:progress="1"
        app:layout_constraintLeft_toRightOf="@+id/txtEducationV"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/houseAnswer" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="88dp"
        android:layout_height="21dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/cooking"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.352"
        app:layout_constraintLeft_toRightOf="@+id/cookAnswer"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.615" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="102dp"
        android:layout_height="21dp"
        android:layout_marginBottom="32dp"
        android:layout_marginLeft="9dp"
        android:layout_marginRight="8dp"
        android:text="@string/entertainment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintLeft_toRightOf="@+id/entertainAnswer"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.058" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="99dp"
        android:layout_height="21dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/education"
        app:layout_constraintHorizontal_bias="0.416"
        app:layout_constraintLeft_toRightOf="@+id/educateAnswer"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="89dp"
        android:layout_height="21dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/house"
        app:layout_constraintHorizontal_bias="0.357"
        app:layout_constraintLeft_toRightOf="@+id/houseAnswer"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/txtHouseV"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="23dp"
        android:text="@string/zero"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/txtHouseQV"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:text="@string/zero"
        app:layout_constraintHorizontal_bias="0.027"
        app:layout_constraintLeft_toRightOf="@+id/houseQuery"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/txtCookingQV" />

    <TextView
        android:id="@+id/txtCookingQV"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="32dp"
        android:text="@string/zero"
        app:layout_constraintHorizontal_bias="0.027"
        app:layout_constraintLeft_toRightOf="@+id/cookQuery"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtEducationQV"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/zero"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintLeft_toRightOf="@+id/educateQuery"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/txtHouseQV" />

    <TextView
        android:id="@+id/txtEntertainQV"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/zero"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintLeft_toRightOf="@+id/entertainQuery"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/txtEducationQV" />

    <TextView
        android:id="@+id/txtEducationV"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="22dp"
        android:text="@string/zero"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/txtEntertainV"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginBottom="32dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/zero"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.144" />

    <TextView
        android:id="@+id/txtCookingV"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="104dp"
        android:text="@string/zero"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button" />

</android.support.constraint.ConstraintLayout>
